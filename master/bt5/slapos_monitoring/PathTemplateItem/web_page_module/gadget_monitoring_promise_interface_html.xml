\n
    <script id="promise-widget-template" type="text/x-handlebars-template">\n
      {{#with element}}\n
        <h2><i class="fa fa-cube"></i><strong> Promise Overview</strong></h2>\n
        <table data-role="table" data-mode="columntoggle" class="ui-responsive table-stroke">\n
          <tr>\n
            <th><i class="fa fa-tachometer"></i> Status</th>\n
            <td><span class="label label-{{status}}">{{status}}</span></td>\n
          </tr>\n
          <tr>\n
            <th><i class="fa fa-calendar"></i> Status Date</th>\n
            <td>{{status_date}}</td>\n
          </tr>\n
          <tr>\n
            <th><i class="fa fa-clock-o"></i> Report Date</th>\n
            <td>{{start-date}}</td>\n
          </tr>\n
        </table>\n
        {{#if message}}\n
        <div style="border: 1px solid rgba(0,0,0,0.1); margin-top: 10px;">\n
          <div class="ui-body">\n
            <p><strong>{{status}}</strong>: {{message}}</p>\n
          </div>\n
        </div>\n
        {{/if}}\n
        <br/>\n
      {{/with}}\n
    </script>\n
    \n
    <script id="pinstance-widget-template" type="text/x-handlebars-template">\n
      <h2><i class="fa fa-cubes"></i><strong> Software Instance Overview</strong></h2>\n
      <div class="ui-field-contain">\n
        <fieldset data-role="controlgroup" data-type="horizontal">\n
          {{#if resource_url }}\n
          <a href="{{resource_url}}" class="ui-btn ui-corner-all ui-btn-inline ui-mini"><i class="fa fa-pie-chart"></i> Resources</a>\n
          {{/if}}\n
          {{#if process_url }}\n
          <a href="{{process_url}}" class="ui-btn ui-corner-all ui-btn-inline ui-mini"><i class="fa fa-list-alt"></i> Processes</a>\n
          {{/if}}\n
        </fieldset>\n
      </div>\n
      <table data-role="table" data-mode="columntoggle" class="ui-responsive table-stroke">\n
        <tr>\n
          <th><i class="fa fa-puzzle-piece"></i> App Title</th>\n
          <td>{{root_title}}</td>\n
        </tr>\n
        <tr>\n
          <th><i class="fa fa-cube"></i> Instance</th>\n
          <td>{{title}}</td>\n
        </tr>\n
        <tr>\n
            <th><i class="fa fa-tachometer"></i> Status</th>\n
            <td><span class="label label-{{status}}">{{status}}</span></td>\n
          </tr>\n
        <tr>\n
          <th><i class="fa fa-clock-o"></i> Report Date</th>\n
          <td>{{date}}</td>\n
        </tr>\n
        <tr>\n
          <th>Errors Ratio</th>\n
          <td>{{errors}} %</td>\n
        </tr>\n
        <tr>\n
          <th>Success Ratio</th>\n
          <td>{{success}} %</td>\n
        </tr>\n
        <tr>\n
          <th>Warning Ratio</th>\n
          <td>{{warning}} %</td>\n
        </tr>\n
      </table>\n
      {{#with instance}}\n
      <br/>\n
      <h2><i class="fa fa-cubes"></i><strong> Software Instance Info</strong></h2>\n
      <table data-role="table" data-mode="columntoggle" class="ui-responsive table-stroke">\n
        <tr>\n
          <th>Computer Reference</th>\n
          <td>{{computer}}</td>\n
        </tr>\n
        <tr>\n
            <th>Computer Partition</th>\n
            <td>{{partition}}</td>\n
          </tr>\n
        <tr>\n
          <th>IPv4</th>\n
          <td>{{ipv4}}</td>\n
        </tr>\n
        <tr>\n
          <th>IPv6</th>\n
          <td>{{ipv6}}</td>\n
        </tr>\n
        <tr>\n
          <th>Software Type</th>\n
          <td>{{software-type}}</td>\n
        </tr>\n
        <tr>\n
          <th>Software Release</th>\n
          <td><a href="{{software-release}}" title="{{software-release}}" target=\'_blank\'>Click to open <i class="fa fa-external-link"></i></a></td>\n
        </tr>\n
      </table>\n
      {{/with}}\n
    </script>\n
\n
    <script id="plinks-widget-template" type="text/x-handlebars-template">\n
      <h2><i class="fa fa-link"></i> <strong>Links</strong></h2>\n
      <div class="ui-field-contain">\n
        <fieldset data-role="controlgroup" data-type="horizontal">\n
          {{#if public_url}}\n
          <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{public_url}}" title="Public"><i class="fa fa-folder-open-o"></i> Public</a>\n
          {{/if}}\n
          {{#if private_url}}\n
          <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{private_url}}" title="Private"><i class="fa fa-lock"></i> Private</a>\n
          {{/if}}\n
          {{#if rss_url}}\n
          <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{rss_url}}" title="RSS"><i class="fa fa-rss-square"></i> RSS</a>\n
          {{/if}}\n
        </fieldset>\n
      </div>\n
    </script>\n
\n
    <script id="phistory-widget-template" type="text/x-handlebars-template">\n
      {{#if history_list}}\n
      <h2><i class="fa fa-history"></i> <strong>Promise Status History</strong></h2>\n
      <div style="max-height: 200px; overflow: auto;">\n
        <table data-role="table" data-mode="columntoggle" class="ui-responsive table-stroke">\n
          {{#each history_list}}\n
          <tr title="{{message}}">\n
            <td style="text-align: center;"><i class="fa fa-circle status-text-{{status}}"></i></td>\n
            <td style="text-align: center;">{{status}}</td>\n
            <td>{{start-date}}</td>\n
          </tr>\n
          {{/each}}\n
        </table>\n
      </div>\n
      {{/if}}\n
    </script>\n
