<!DOCTYPE html>\n
<html>\n
  <head>\n
    <meta charset="utf-8" />\n
    <meta name="viewport" content="width=device-width, user-scalable=no" />\n
    <title>Monitoring Widget Listbox</title>\n
\n
    <!-- renderjs -->\n
    <script src="rsvp.js" type="text/javascript"></script>\n
    <script src="renderjs.js" type="text/javascript"></script>\n
    <script src="handlebars.js"></script>\n
\n
    <script id="overview-widget-listview" type="text/x-handlebars-template">\n
      <div class="ui-listview-container">\n
        <table class="ui-responsive ui-body-c ui-table-inset instance-overview">\n
          <thead class="ui-bar-inherit">\n
            <tr>\n
              {{#each column_list}}\n
                <th>{{title}}</th>\n
              {{/each}}\n
            </tr>\n
          </thead>\n
          <tbody>\n
          {{#each row_list}}\n
            <tr rel="{{index}}">\n
              <td class="ui-text-center"><a class="ui-link" href="{{href}}">\n
                <span class="ui-status-icon ui-status-{{status}}"></span></a></td>\n
              <td><a class="ui-link" href="{{href}}">{{value}}</a></td>\n
              <td><a class="ui-link" href="{{href}}">{{hosting_value}}</a></td>\n
            </tr>\n
          {{/each}}\n
          </tbody>\n
          <tfoot class="ui-bar-inherit"></tfoot>\n
        </table>\n
      </div>\n
    </script>\n
\n
    <script id="header-widget-overview" type="text/x-handlebars-template">\n
      <div class="header-form-overview">\n
        <form class=\'search\'>\n
          <div class="ui-grid-a ui-responsive">\n
            <div class="ui-block-a">\n
              <div class="ui-field-contain">\n
                <div class="ui-input-text ui-body-{{widget_theme}} ui-corner-all ui-shadow-inset ui-input-has-clear {{widget_status_class}}" style="border-color: #D6D8E0;">\n
                  <input type="text" data-enhanced="true" value="{{search}}" name="search" {{widget_status_attribute}} />\n
                  <div class="ui-btn ui-input-clear ui-input-btn ui-corner-all ui-icon-search ui-btn-icon-notext" data-i18n="[node]header-submit">\n
                    Submit<input data-enhanced="true" type="submit" data-i18n="[value]header-submit" value="Submit" {{widget_status_attribute}} />\n
                  </div>\n
                </div>\n
              </div>\n
            </div>\n
            <div class="ui-block-b">\n
              <div class="ui-field-contain">\n
                <div data-role="controlgroup" data-type="horizontal">\n
                  <a href="#" class="ui-shadow ui-btn ui-corner-all listview-refresh"><i class="fa fa-refresh"></i></a>\n
                  <a href="#" class="ui-shadow ui-btn ui-corner-all listview-filter"><i class="fa fa-cog"></i></a>\n
                </div>\n
              </div>\n
            </div>\n
          </div>\n
        </form>\n
      </div>\n
    </script>\n
\n
    <script id="details-widget-overview" type="text/x-handlebars-template">\n
      <div class="overview-header">\n
        <div class=\'content-title ui-instance-title\'>\n
          <div class="overview-title">\n
            <i class="fa fa-cubes"></i><span>\n
              {{title}}\n
              {{#if root_title}}\n
              &nbsp; [{{root_title}}]\n
              {{/if}}\n
              </span>\n
          </div>\n
          <div class="commands">\n
            \n
          </div>\n
        </div>\n
      </div>\n
      <div class="content-details">\n
        <div class="ui-text-left" style="padding-bottom: 15px;">\n
