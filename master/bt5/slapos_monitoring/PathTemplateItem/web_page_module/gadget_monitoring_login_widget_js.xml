            <key> <string>text_content</string> </key>
            <value> <string encoding="cdata"><![CDATA[

/*global document, window, rJS */\n
/*jslint nomen: true, indent: 2, maxerr: 3*/\n
(function (window, document, rJS) {\n
  "use strict";\n
\n
  var gadget_klass = rJS(window),\n
    hashCode;\n
\n
  hashCode = function (s) {\n
    return s.split("").reduce(function(a,b){a=((a<<5)-a)+b.charCodeAt(0);return a&a},0);              \n
  };\n
\n
  gadget_klass\n
    .ready(function (g) {\n
      g.props = {};\n
      g.props.from = {page: \'main\'};\n
      g.props.login = \'login\';\n
    })\n
    .declareAcquiredMethod("redirect", "redirect")\n
    .declareAcquiredMethod(\'getUrlFor\', \'getUrlFor\')\n
    .declareAcquiredMethod("getSetting", "getSetting")\n
    .declareAcquiredMethod("setSetting", "setSetting")\n
\n
    .declareMethod(\'login\', function (url, credential_hash) {\n
      return new RSVP.Promise(function(resolve, reject){\n
        var xhr = new XMLHttpRequest();\n
\n
        \n
        xhr.onload = function (event) {\n
          var response = event.target;\n
          if (response.status === 200) {\n
            resolve({status: \'OK\'});\n
          } else {\n
            reject(new Error("XHR: " + response.status + ": " + response.statusText));\n
          }\n
        };\n
        xhr.onerror = function (e) {\n
          reject({status: \'ERROR\', msg: e.target.status + ": " + e.target.statusText});\n
        };\n
        xhr.open("GET", url, true);\n
        //xhr.withCredentials = true;\n
        xhr.setRequestHeader(\'Authorization\', \'Basic \' + credential_hash);\n
        //try {\n
        xhr.send("");\n
        //} catch (e) {\n
        //  reject({status: \'ERROR\', msg: e});\n
        //}\n
      });\n
    })\n
    .declareMethod(\'loginRedirect\', function (url, options_dict, title, root_title) {\n
      var gadget = this,\n
        redirectWith,\n
        hash_url = hashCode(url) + \'\';\n
        \n
      redirectWith = function (option_dict) {\n
        return gadget.setSetting(\'redirect_after_login\', options_dict)\n
          .push(function () {\n
            return gadget.redirect({\n
              page: gadget.props.login,\n
              url: url,\n
              title: title || \'\',\n
              root: root_title || \'\'\n
            });\n
          });\n
      };\n
\n
      return gadget.getSetting(hash_url)\n
        .push(function (credential_dict) {\n
          if (!credential_dict) {\n
            credential_dict = {hash: btoa(\'no:password\'), url: url};\n
          }\n
          return credential_dict;\n
        })\n
        .push(function (credential) {\n
          return gadget.login(credential.url, credential.hash)\n
            .then(function(result) {\n
              if (result.status === \'OK\') {\n
                return credential;\n
              } else {\n
                return redirectWith(options_dict);\n
              }\n
            }, function(error) {\n
              return redirectWith(options_dict);\n
            });\n
        });\n
    });\n
\n
}(window, document, rJS));

]]></string> </value>
