            <key> <string>text_content</string> </key>
            <value> <string encoding="cdata"><![CDATA[

/*global document, window, rJS */\n
/*jslint nomen: true, indent: 2, maxerr: 3*/\n
(function (window, document, rJS) {\n
  "use strict";\n
\n
  var gadget_klass = rJS(window),\n
    templater = gadget_klass.__template_element,\n
\n
    llogin_tooltip_template = Handlebars.compile(\n
      templater.getElementById("login-toltip-template").innerHTML\n
    ),\n
    hashCode;\n
\n
  hashCode = function (s) {\n
    return s.split("").reduce(function(a,b){a=((a<<5)-a)+b.charCodeAt(0);return a&a},0);              \n
  };\n
\n
  gadget_klass\n
    .ready(function (g) {\n
      g.props = {};\n
      \n
      return g.getElement()\n
        .push(function (element) {\n
          g.props.element = element;\n
          g.props.deferred = RSVP.defer();\n
          g.props.from = {page: \'main\'};\n
          g.props.login = \'login\';\n
        });\n
    })\n
    .declareAcquiredMethod("redirect", "redirect")\n
    .declareAcquiredMethod(\'getUrlFor\', \'getUrlFor\')\n
    .declareAcquiredMethod("getSetting", "getSetting")\n
    .declareAcquiredMethod("setSetting", "setSetting")\n
    .declareMethod("render", function (options) {\n
      var gadget = this,\n
        tmp;\n
\n
      if (!options.url) {\n
        tmp = document.URL.split(\'#\');\n
        if (tmp.length > 1) {\n
          gadget.props.url = tmp[0];\n
        } else {\n
          gadget.props.url = "";\n
        }\n
      } else {\n
        gadget.props.url = options.url;\n
      }\n
      gadget.props.title = options.title || \'Please sign in to Monitoring\';\n
      gadget.props.root = options.root || \'UNKNOW\';\n
      return gadget.getSetting(\'redirect_after_login\')\n
        .push(function (redirect_after_login) {\n
          if (redirect_after_login) {\n
            gadget.props.from = redirect_after_login;\n
          }\n
          gadget.props.element.querySelector(\'h3\')\n
            .innerHTML = \'<i class="fa fa-cube"></i> \' + gadget.props.title;\n
          return gadget.props.deferred.resolve();\n
        });\n
    })\n
\n
    /////////////////////////////////////////\n
    // Form submit\n
    /////////////////////////////////////////\n
    .declareService(function () {\n
      var gadget = this;\n
\n
      return new RSVP.Queue()\n
        .push(function () {\n
          return gadget.props.deferred.promise;\n
        })\n
        .push(function (redirect_after_login) {\n
          var text = \'Open Link\',\n
            content = llogin_tooltip_template({\n
              title: gadget.props.title,\n
              root_title: gadget.props.root,\n
              href: gadget.props.url,\n
              link_title: text\n
            });\n
\n
          return $(\'.tooltip\').tooltipster({\n
            animation: \'fade\',\n
            delay: 200,\n
            theme: \'tooltipster-shadow\',\n
            touchDevices: true,\n
            content: $(content),\n
            interactive: true,\n
            trigger: \'hover\'\n
          });\n
        })\n
        .push(function () {\n
          return loopEventListener(\n
            gadget.props.element.querySelector(\'form\'),\n
            \'submit\',\n
            true,\n
            function () {\n
              var username = gadget.props.element.querySelector("input[name=\'username\']").value,\n
                password = gadget.props.element.querySelector("input[name=\'password\']").value;\n
\n
              return gadget.getDeclaredGadget(\'login_gadget\')\n
                .push(function (login_gadget) {\n
                    var hash = btoa(username + \':\' + password);\n
                    return login_gadget.login(gadget.props.url, hash)\n
                      .then(function(result) {\n
                        if (result.status === \'OK\') {\n
                          return gadget.setSetting(\'redirect_after_login\', \'\')\n
                            .push(function () {\n
                              var credential = {\n
                                  hash: hash,\n
                                  url: gadget.props.url\n
                                },\n
                                hash_url = hashCode(gadget.props.url) + \'\';\n
                              return gadget.setSetting(hash_url, credential);\n
                            })\n
                            .push(function () {\n
                              return gadget.redirect(gadget.props.from);\n
                            });\n
                        } else {\n
                          gadget.props.element.querySelector(".msg")\n
                            .innerHtml = \'<span class="ui-text-\' + error.status +\n
                              \'">Invalid username or password.</span>\';\n
                        }\n
                      }, function(error) {\n
                        gadget.props.element.querySelector(".msg")\n
                          .innerHTML =\'<span class="ui-text-\' + error.status +\n
                              \'">Invalid username or password.</span>\';\n
                      });\n
                });\n
            }\n
          );\n
        });\n
    });\n
\n
}(window, document, rJS));

]]></string> </value>
