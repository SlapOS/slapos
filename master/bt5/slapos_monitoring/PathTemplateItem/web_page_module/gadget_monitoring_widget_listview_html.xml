<!DOCTYPE html>\n
<html>\n
  <head>\n
    <meta charset="utf-8" />\n
    <meta name="viewport" content="width=device-width, user-scalable=no" />\n
    <title>Monitoring Widget Listbox</title>\n
\n
    <!-- renderjs -->\n
    <script src="rsvp.js" type="text/javascript"></script>\n
    <script src="renderjs.js" type="text/javascript"></script>\n
    <script src="handlebars.js"></script>\n
\n
    <script id="header-widget-listbox" type="text/x-handlebars-template">\n
      <form class=\'search\'>\n
        <div class="ui-field-contain">\n
          <div class="ui-input-text ui-body-{{widget_theme}} ui-corner-all ui-shadow-inset ui-input-has-clear {{widget_status_class}}">\n
            <input type="text" data-enhanced="true" value="{{search}}" name="search" {{widget_status_attribute}} />\n
            <div class="ui-btn ui-input-clear ui-input-btn ui-corner-all ui-icon-search ui-btn-icon-notext" data-i18n="[node]header-submit">\n
              Submit<input data-enhanced="true" type="submit" data-i18n="[value]header-submit" value="Submit" {{widget_status_attribute}} />\n
            </div>\n
          </div>\n
        </div>\n
        <div class="ui-field-contain">\n
          <div data-role="controlgroup" data-type="horizontal">\n
            <a href="#" class="ui-shadow ui-btn ui-corner-all listview-refresh"><i class="fa fa-refresh"></i></a>\n
            <a href="#" class="ui-shadow ui-btn ui-corner-all listview-filter"><i class="fa fa-filter"></i></i></a>\n
            <input type="radio" name="listview-sort-radio" id="listview-sort-title" value="title">\n
            <label for="listview-sort-title" data-i18n="Name">Name <i class="fa fa-sort-alpha-asc"></i></label>\n
            <input type="radio" name="listview-sort-radio" id="listview-sort-status" value="status">\n
            <label for="listview-sort-status" data-i18n="Status">Status <i class="fa fa-sort-amount-asc"></i></label>\n
          </div>\n
        </div>\n
      </form>\n
    </script>\n
\n
    <script id="promise-widget-listbox" type="text/x-handlebars-template">\n
      <div class="ui-listview-container">\n
        {{#with header}}\n
          {{#if title}}\n
          <h3 class=""><strong>{{title}}</strong></h3>\n
          {{/if}}\n
          <div class="ui-field-contain">\n
            <fieldset data-role="controlgroup" data-type="horizontal">\n
              {{#if public_url}}\n
              <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{public_url}}" title="Public"><i class="fa fa-folder-open-o"></i></a>\n
              {{/if}}\n
              {{#if private_url}}\n
              <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{private_url}}" title="Private"><i class="fa fa-lock"></i></a>\n
              {{/if}}\n
              {{#if rss_url}}\n
              <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" target=\'_blank\' href="{{rss_url}}" title="RSS"><i class="fa fa-rss-square"></i></a>\n
              {{/if}}\n
            </fieldset>\n
          </div>\n
        {{/with}}\n
        <ul data-role="listview" class="ui-listview-outer" data-inset="true">\n
          {{#each row_list}}\n
            <li>\n
              <a href="#" class="ui-btn ui-btn-icon-right ui-icon-plus">\n
              <span class="ui-status-icon ui-status-{{status}}"></span>\n
              <span class="ui-status-text">{{value}}</span>\n
              </a>\n
              <div class="ui-collapse-content ui-content-hidden">\n
                <fieldset>\n
                  {{#each data_list}}\n
                    {{#if text_value}}\n
                      {{#if inline}}\n
                        <span><i class="{{icon_class}}"></i> {{text_value}}</span>\n
                      {{else}}\n
                        <div class="{{class}}">\n
                          <span><i class="{{icon_class}}"></i> {{text_value}}\n
                          </span>\n
                        </div>\n
                      {{/if}}\n
                    {{/if}}\n
                  {{/each}}\n
                  <div class="ui-field-contain">\n
                    <fieldset data-role="controlgroup" data-type="horizontal">\n
                      <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" href="{{href}}"><i class="fa fa-external-link"></i></a>\n
                      <a class="ui-btn ui-corner-all ui-btn-inline ui-mini" href="{{href}}"><i class="fa fa-folder-open-o"></i></a>\n
                    </fieldset>\n
                  </div>\n
                </fieldset>\n
              </div>\n
            </li>\n
          {{/each}}\n
        </ul>\n
      </div>\n
    </script>\n
\n
    <!-- custom script -->\n
    <script src="gadget_monitoring_widget_listview.js" type="text/javascript"></script>\n
\n
    </head>\n
  <body>\n
    <input type="hidden" name="listbox-block-indexes", value="" />\n
    <div data-gadget-url="gadget_monitoring_jio.html" data-gadget-scope="jio_gadget" data-gadget-sandbox="public"></div>\n
    <div class="listbox-filter-panel">\n
      <form class=\'filter\'>\n
      \t<h2>Filter Result</h2>\n
      \t<fieldset data-role="controlgroup">\n
          <h3>Status to show:</h3>\n
          <input type="checkbox" id="monitor-promise-success" name="monitor-filter-status" value="success" checked="checked">\n
          <label for="monitor-promise-success">Success</label>\n
          <input type="checkbox" id="monitor-promise-error" name="monitor-filter-status" value="error" checked="checked">\n
          <label for="monitor-promise-error">Error</label>\n
          <input type="checkbox" id="monitor-promise-warning" name="monitor-filter-status" value="warning" checked="checked">\n
          <label for="monitor-promise-warning">Warning</label>\n
        </fieldset>\n
        <div class="ui-grid-a">\n
          <input data-enhanced="true" type="submit" data-rel="close" class="ui-btn ui-shadow ui-corner-all ui-btn-inline ui-mini pvalidate" value="Validate" />\n
\t      </div>\n
      </form>\n
    </div>\n
    <div class="center"></div>\n
    <div class="ui-grid-a ui-responsive custom-listbox">\n
      <div class="ui-block-a"></div>\n
      <div class="ui-block-b"></div>\n
    </div>\n
  </body>\n
</html>\n


