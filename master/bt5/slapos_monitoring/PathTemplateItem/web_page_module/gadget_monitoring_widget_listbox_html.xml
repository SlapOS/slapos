<!DOCTYPE html>\n
<html>\n
  <head>\n
    <meta charset="utf-8" />\n
    <meta name="viewport" content="width=device-width, user-scalable=no" />\n
    <title>Monitoring Widget Listbox</title>\n
\n
    <!-- renderjs -->\n
    <script src="rsvp.js" type="text/javascript"></script>\n
    <script src="renderjs.js" type="text/javascript"></script>\n
    <script src="handlebars.js"></script>\n
\n
    <script id="listbox-widget-table" type="text/x-handlebars-template">\n
      {{#if enable_search}}\n
      <div class="listbox-header">\n
        <form class=\'search\'>\n
            <div class="ui-grid-a ui-responsive">\n
              <div class="ui-block-a">\n
                <div class="ui-field-contain">\n
                  <div class="ui-input-text ui-body-{{widget_theme}} ui-corner-all ui-shadow-inset ui-input-has-clear {{widget_status_class}}" style="border-color: #D6D8E0;">\n
                    <input type="text" data-enhanced="true" value="{{search}}" name="search" {{widget_status_attribute}} />\n
                    <div class="ui-btn ui-input-clear ui-input-btn ui-corner-all ui-icon-search ui-btn-icon-notext" data-i18n="[node]header-submit">\n
                      Submit<input data-enhanced="true" type="submit" data-i18n="[value]header-submit" value="Submit" {{widget_status_attribute}} />\n
                    </div>\n
                  </div>\n
                </div>\n
              </div>\n
              <div class="ui-block-b">\n
                <div class="ui-field-contain">\n
                  <div data-role="controlgroup" data-type="horizontal">\n
                    <a href="#" class="ui-shadow ui-btn ui-corner-all listbox-refresh"><i class="fa fa-refresh"></i></a>\n
                    <a href="#" class="ui-shadow ui-btn ui-corner-all listbox-filter"><i class="fa fa-filter"></i></i></a>\n
                  </div>\n
                </div>\n
              </div>\n
            </div>\n
        </form>\n
      </div>\n
      {{/if}}\n
      <table class="ui-responsive ui-body-c ui-table-inset custom-force-list">\n
        <thead class="ui-bar-inherit">\n
          <tr>\n
            {{#each column_list}}\n
              <th>{{this}}</th>\n
            {{/each}}\n
          </tr>\n
        </thead>\n
        {{> listbox-widget-table-partial}}\n
        <tfoot class="ui-bar-inherit"></tfoot>\n
      </table>\n
    </script>\n
\n
    <script id="listbox-widget-table-partial" type="text/x-handlebars-template">\n
      <tbody>\n
      {{#each row_list}}\n
        <tr>\n
        {{#each cell_list}}\n
          {{#if href}}\n
            {{#if html_value}}\n
              <td class=\'{{class}}\'><a class="ui-link" href="{{href}}">{{{html_value}}}</a></td>\n
            {{else}}\n
              <td class=\'{{class}}\'><a class="ui-link" title=\'{{value}}\' href="{{href}}">{{value}}</a></td>\n
            {{/if}}\n
          {{else}}\n
            {{#if html_value}}\n
              <td class=\'{{class}}\'><a class="ui-link">{{{html_value}}}</a></td>\n
            {{else}}\n
              <td class=\'{{class}}\'><a class="ui-link" title=\'{{value}}\'>{{value}}</a></td>\n
            {{/if}}\n
          {{/if}}\n
        {{/each}}\n
        </tr>\n
      {{/each}}\n
      </tbody>\n
    </script>\n
\n
    <!-- custom script -->\n
    <script src="gadget_monitoring_widget_listbox.js" type="text/javascript"></script>\n
\n
    </head>\n
  <body>\n
    <div data-gadget-url="gadget_monitoring_jio.html" data-gadget-scope="jio_gadget" data-gadget-sandbox="public"></div>\n
    <div class="listbox-filter-panel">\n
      <form class=\'filter\'>\n
      \t<h2>Filter Result</h2>\n
      \t<fieldset data-role="controlgroup">\n
          <h3>Status contain:</h3>\n
          <input type="checkbox" id="monitor-promise-success" name="monitor-filter-status" value="success" checked="checked">\n
          <label for="monitor-promise-success">Success</label>\n
          <input type="checkbox" id="monitor-promise-error" name="monitor-filter-status" value="error" checked="checked">\n
          <label for="monitor-promise-error">Error</label>\n
          <input type="checkbox" id="monitor-promise-warning" name="monitor-filter-status" value="warning" checked="checked">\n
          <label for="monitor-promise-warning">Warning</label>\n
        </fieldset>\n
        <div class="ui-grid-a">\n
          <input data-enhanced="true" type="submit" data-rel="close" class="ui-btn ui-shadow ui-corner-all ui-btn-inline ui-mini pvalidate" value="Validate" />\n
\t      </div>\n
      </form>\n
    </div>\n
    <div class="custom-grid-wrap">\n
      <div class="custom-grid ui-corner-all ui-body-inherit ui-shadow ui-corner-all" style="padding: 10px;">\n
        <div class="ui-body-c ui-corner-all"></div>\n
      </div>\n
    </div>\n
  </body>\n
</html>\n


