{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "description": "Values returned by Selenium instanciation",
  "properties": {
    "backend-url": {
      "description": "Selenium hub URL",
      "type": "string"
    },
    "url": {
      "description": "Selenium instance URL",
      "type": "string"
    },
    "admin-url": {
      "description": "Selenium admin URL",
      "type": "string"
    },
    "frontend-url": {
      "description": "Selenium frontend URL",
      "type": "string"
    },
    "user": {
      "description": "Selenium username",
      "type": "string"
    },
    "pass": {
      "description": "Selenium password",
      "type": "string"
    }
  }
}
