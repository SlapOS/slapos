{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Values returned by Theia instanciation",
  "properties": {
    "url": {
      "description": "Theia Frontend URL (private)",
      "type": "string"
    },
    "url-public": {
      "description": "Theia Public URL (to serve static files, public)",
      "type": "string"
    },
    "url-slapproxy": {
      "description": "Theia Slapproxy URL (to see the panel GUI, private)",
      "type": "string"
    },
    "username": {
      "description": "Theia username",
      "type": "string"
    },
    "password": {
      "description": "Theia password",
      "type": "string"
    },
    "backend-url": {
      "description": "Theia Backend URL",
      "type": "string"
    },
    "ipv6": {
      "description": "A Theia ipv6 address",
      "type": "string"
    }
  },
  "type": "object"
}
