{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Input Parameters",
  "properties": {
    "tx_gain": {
      "title": "Tx gain",
      "description": "Tx gain (in dB)",
      "type": "number",
      "default": 70
    },
    "rx_gain": {
      "title": "Rx gain",
      "description": "Rx gain (in dB)",
      "type": "number",
      "default": 30
    },
    "dl_earfcn": {
      "title": "DL EARFCN",
      "description": "Downlink E-UTRA Absolute Radio Frequency Channel Number of the cell",
      "type": "number",
      "default": 42590
    },
    "n_rb_dl": {
      "title": "DL RB",
      "description": "number of DL resource blocks",
      "type": "number",
      "default": 100
    },
    "mme_addr": {
      "title": "MME Address",
      "description": "IPv4 of the core network",
      "type": "string",
      "default": "127.0.0.100"
    },
    "enb_id": {
      "title": "eNB ID",
      "description": "eNB ID",
      "type": "string",
      "default": "0x1A2D0"
    }
  }
}