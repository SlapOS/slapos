{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "description": "Parameters to request selenium instance",
  "additionalProperties": false,
  "properties": {
    "ssh-authorized-key": {
      "title": "SSH Key",
      "description": "ssh public key",
      "type": "string",
      "default": ""
    },
    "slotQuantity": {
      "title": "Slot quantity",
      "description": "Number of slot per selenium node",
      "type": "integer",
      "default": 3
    }
  }
}
