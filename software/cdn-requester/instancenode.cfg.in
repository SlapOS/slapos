requestinstance-db-path = $${cdn-requester-configuration:requestinstance-db-path}
software-url = $${slap-configuration:configuration.cdn-software-url}
software-type = $${slap-configuration:configuration.cdn-software-type}
-sla-computer_guid = $${slap-configuration:configuration.-sla-computer_guid}
