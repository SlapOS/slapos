{
    "configuration": {
        "service": {
            "flush": 5,
            "daemon": "off",
            "log_level": "debug"
        },
        "pipeline": {
            "inputs": [
                {
                    "cpu": {
                        "tag": "my_cpu"
                    }
                }
            ],
            "outputs": [
                {
                    "stdout": {
                        "match": "my*cpu"
                    }
                }
            ]
        }
    }
}
