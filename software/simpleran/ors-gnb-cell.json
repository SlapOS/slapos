{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ORS NR Cell",
  "type": "object",
  "properties": {
    "nr_bandwidth": {
      "title": "Bandwidth",
      "description": "Downlink Bandwidth",
      "enum": [
        "20 MHz",
        "40 MHz"
      ],
      "default": "40 MHz",
      "type": "string"
    },
    "dl_frequency": {
      "title": "DL Frequency",
      "description": "Downlink center frequency of the cell. (Range: 4610.01 MHz - 4989.99 MHz with 20 MHz bandwidth, or 4620 MHz - 4979.985 MHz with 40 MHz bandwidth)",
      "type": "number"
    },
    "tdd_ul_dl_config": {
      "$ref": "cell/nr/input-schema.json#/properties/tdd_ul_dl_config"
    }
  }
}
